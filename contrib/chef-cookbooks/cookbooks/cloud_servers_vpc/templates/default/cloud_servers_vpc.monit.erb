check process delayed_job
  with pidfile <%= node[:cloud_servers_vpc][:home] %>/tmp/pids/delayed_job.0.pid
  start program = "/usr/bin/env RAILS_ENV=production <%= node[:ruby_enterprise][:ruby_bin] %> /opt/cloud-servers-vpc/script/delayed_job start -n <%= node[:cloud_servers_vpc][:delayed_job_workers] %>"
  stop program = "<%= node[:cloud_servers_vpc][:home] %>/contrib/bin/cloud-servers-vpc-stop-dj"
